SUMMARY="Script to download and install dlls needed to run some programs in wine"
HOMEPAGE="http://$PN.org/"
DOWNLOADS="http://$PN.org/$PN"

SLOT="0"
PLATFORMS="~amd64 ~x86"

src_unpack() {
    echo $ARCHIVES
    echo $WORKBASE
    ls -la
    cp $ARCHIVES $WORKBASE
}

WORK=$WORKBASE

src_install() {
    dobin $PN
}
