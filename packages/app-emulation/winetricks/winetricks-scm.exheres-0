SUMMARY="Script to download and install dlls needed to run some programs in wine"
HOMEPAGE="http://$PN.org/"
URI="http://$PN.org/$PN"

SLOT="0"
PLATFORMS="~amd64 ~x86"

src_unpack() {
    wget $URI || die
}

src_install() {
    dobin $PN
}
